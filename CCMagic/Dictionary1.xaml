<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CCMagic.ViewModel"
                    xmlns:core="clr-namespace:S3ToolKit.MagicEngine.Core;assembly=S3ToolKit.MagicEngine"
                    
                    >


    <!-- Template for the Game Version Icon -->
    <DataTemplate DataType="{x:Type vm:GameVersionDisplayEntry}">
        <ContentControl Margin="1,0" Content="{Binding Content}" ToolTip="{Binding DisplayValue}" Width="20" Height="20" />
    </DataTemplate>

</ResourceDictionary>